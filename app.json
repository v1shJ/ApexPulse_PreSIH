{
  "expo": {
    "name": "pre-sih-frontend",
    "slug": "pre-sih-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to show you where you are on the map.",
        "NSLocationAlwaysUsageDescription": "We need your location to show you where you are on the map."
      },
      "bundleIdentifier": "geotracker-pre-sih"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "package": "com.ahaan_desai27.presihfrontend"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8e238fc9-bc97-4f02-b448-4c80d136894d"
      }
    }
  }
}
